<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>

<BorderPane prefWidth="300" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8">
	<top>
		<HBox alignment="CENTER" prefHeight="50" spacing="20">
			<children>
				<Label text="제목: " />
				<TextField id="MassageTitle" prefWidth="220.0" />	
			</children>
		</HBox>
	</top>
	<center>
		<TextField id="Massage" prefHeight="150" prefWidth="150" />
	</center>
	<bottom>
		<HBox alignment="CENTER" prefHeight="50" spacing="20">
			<children>
				<Button fx:id="btnSendMassage" text="보내기" />
				<Button fx:id="btnMassageEnd" text="취소" />
			</children>
		</HBox>
	</bottom>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</BorderPane>
